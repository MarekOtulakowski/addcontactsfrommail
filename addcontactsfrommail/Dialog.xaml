<Window x:Class="addcontactsfrommail.Dialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dialog" Height="188" Width="299" HorizontalAlignment="Center" VerticalAlignment="Center" Background="Black">
    <Grid Height="153" Width="278" Background="{x:Null}">
        <Button Content="Yes" Height="23" HorizontalAlignment="Left" Margin="27,97,0,0" Name="B_yes" VerticalAlignment="Top" Width="75" Click="B_yes_Click" />
        <Button Content="No" Height="23" HorizontalAlignment="Right" Margin="0,97,25,0" Name="B_no" VerticalAlignment="Top" Width="75" Click="B_no_Click" />
        <Label Content="null" Height="70" HorizontalAlignment="Left" Margin="27,12,0,0" Name="L_question" VerticalAlignment="Top" Width="226" />
    </Grid>  
</Window>

